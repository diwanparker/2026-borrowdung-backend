# Database Configuration
# SQL Server Connection String
ConnectionStrings__DefaultConnection=Server=localhost;Database=BorrowdungDB;User Id=sa;Password=YourStrong@Passw0rd;TrustServerCertificate=True;

# Alternative: SQLite (for development)
# ConnectionStrings__DefaultConnection=Data Source=borrowdung.db

# Application Settings
ASPNETCORE_ENVIRONMENT=Development
ASPNETCORE_URLS=http://localhost:5000;https://localhost:5001

# CORS Settings
AllowedOrigins=http://localhost:3000,http://localhost:4200

# JWT Settings (if using authentication)
# JWT__Secret=YourSuperSecretKeyHere
# JWT__Issuer=BorrowdungAPI
# JWT__Audience=BorrowdungClient
# JWT__ExpiryInMinutes=60

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft=Warning
Logging__LogLevel__Microsoft.AspNetCore=Warning
